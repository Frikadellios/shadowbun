{
  "name": "shadowbun",
  "type": "module",
  "packageManager": "bun@1.1.26",
  "private": true,
  "keywords": ["template", "biomejs", "bun", "typescript", "tailwindcss", "astrojs", "react"],
  "author": "Hrihorii Ilin <devopsick@pm.me>",
  "description": "To install dependencies: bun install",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frikadellios/shadowbun.git"
  },
  "bugs": {
    "url": "https://github.com/Frikadellios/shadowbun/issues"
  },
  "homepage": "https://github.com/Frikadellios/shadowbun#readme",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "browserslist": "bunx update-browserslist-db@latest",
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun wrangler types && bun astro check --verbose && bunx --bun astro dev --verbose",
    "build": "bun wrangler types && bun astro check --verbose && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro build && bun wrangler pages dev ./dist",
    "astro": "astro",
    "deploy": "bunx --bun astro build && bun wrangler pages deploy ./dist",
    "cf-typegen": "wrangler types",
    "tailwindcss": "bunx @tailwindcss/cli@next -i ./src/styles/app.css -o ./src/styles/dist/app.css",
    "lint": "conc bun:lint:*",
    "lint:format": "bunx @biomejs/biome format . --write",
    "lint:fix": "bunx @biomejs/biome check . --fix",
    "lint:check": "bunx @biomejs/biome check ./* --write --unsafe"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/cloudflare": "^11.0.4",
    "@astrojs/mdx": "^3.1.5",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/ts-plugin": "^1.10.2",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.4",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@react-spring/web": "^9.7.4",
    "@tabler/icons-react": "^3.16.0",
    "@tailwindcss/cli": "^4.0.0-alpha.23",
    "@tailwindcss/postcss": "^4.0.0-alpha.23",
    "@tailwindcss/vite": "^4.0.0-alpha.23",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.15.4",
    "autoprefixer": "^10.4.20",
    "balance-text": "^3.3.1",
    "caniuse-lite": "^1.0.30001660",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cobe": "^0.6.3",
    "concurrently": "^9.0.0",
    "date-fns": "^3.6.0",
    "detect-browser-es": "^0.1.1",
    "embla-carousel-react": "^8.2.1",
    "emblor": "^1.4.6",
    "framer-motion": "^11.5.4",
    "framer-motion-3d": "^11.5.4",
    "lucide-react": "^0.439.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-refresh": "^0.14.2",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^4.0.0-alpha.23",
    "typescript": "^5.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/bun": "latest",
    "@types/html": "^1.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.5.4",
    "@types/web": "^0.0.162",
    "editorconfig-checker": "^5.1.8",
    "wrangler": "^3.76.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": ["@biomejs/biome", "esbuild", "sharp", "workerd"]
}
